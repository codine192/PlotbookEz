        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EzPlot.Views"
        mc:Ignorable="d"
        Title="ViewResidentListPageView" Height="450" Width="800">
    <Grid Background="AliceBlue">
        <DataGrid x:Name="dataGridResidents" d:ItemsSource="{d:SampleData ItemCount=50}" MouseDoubleClick="DataGrid_MouseDoubleClick" Margin="0,5,0,-5"/>


    </Grid>
</Page>
